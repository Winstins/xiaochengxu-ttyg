/**app.wxss**/
@import "./styles/iconfont.wxss";

/* 在小程序中 不支持 通配符'*' */
page,view,textPath,swiper,swiper-item,image,navigator{
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

/* 主题颜色，通过变量来实现 
1 less 中 存在变量这个知识
2 原生的css和wxss也是支持
*/
page{
  /* 定义主体颜色 */
  --themeColor:#eb4450;
}

image{
  width: 100%;
}